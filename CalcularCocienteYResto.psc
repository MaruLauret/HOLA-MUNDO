Algoritmo CalcularCocienteYResiduo
	Numero1 <- 146;
	Numero2 <- 25;
	Resto <- Numero1 MOD Numero2;
	Cociente <- (Numero1 - Resto) / Numero2;
	Mostrar "El cociente es ", Cociente;
	Mostrar "El resto es ", Resto;
FinAlgoritmo
//que calcule el cociente y el residuo de la división de 146 dividido 25